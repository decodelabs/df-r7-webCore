{
    "name": "df-r7/webcore",
    "description": "DF r7 webCore",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "repositories": [{
        "type": "vcs",
        "url": "git@github.com:decodelabs/df-r7-base.git"
    }],
    "require": {
        "df-r7/base": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "1.8.x-dev",
        "phpstan/extension-installer": "^1.0",

        "decodelabs/phpstan-decodelabs": "^0.6",
        "symplify/easy-coding-standard": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\": "provider/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\PHPStan\\": "vendor/df-r7/base/phpstan/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev"
        },
        "effigy": {
            "codeDirs": [
                "directory",
                "helpers",
                "hooks",
                "libraries",
                "models",
                "provider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
